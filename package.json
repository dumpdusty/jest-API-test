{
  "name": "paysis-jest",
  "version": "1.0.0",
  "description": "api tests for paysis project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "Dump Dusty",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "7.22.5",
    "axios": "1.4.0",
    "babel-plugin-module-resolver": "5.0.0",
    "body": "5.1.0",
    "body-parser": "1.20.2",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "jest": "29.5.0",
    "parser": "0.1.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "prettier": "2.8.8"
  },
  "jest": {
    "testTimeout": 20000,
    "maxWorkers": 1,
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "modulePathIgnorePatterns": [
      "example.spec.js"
    ]
  }
}
